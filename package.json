{
  "name": "generator-sublime",
  "version": "0.2.0",
  "description": "Yeoman generator for scaffolfding the standard configuration root files like .gitignore, .jshintrc, .jscsrc etc...",
  "license": "MIT",
  "main": "app/index.js",
  "repository": "thaiat/generator-sublime",
  "author": {
    "name": "Avi Haiat",
    "email": "thaiat@ipelia.com",
    "url": "https://github.com/thaiat"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "prepublish": "bash ./deploy/prepublish.sh",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha ./test && istanbul report cobertura",
    "test": "gulp lint && mocha ./test"
  },
  "files": [
    "app",
    "templates"
  ],
  "preferGlobal": true,
  "keywords": [
    "yeoman-generator",
    "editor",
    "app",
    "sublime",
    "lint",
    "jshint",
    "jsbeautify",
    "tern",
    "scaffold"
  ],
  "dependencies": {
    "chalk": "~0.4.0",
    "github": "^0.2.1",
    "lodash": "^2.4.1",
    "strip-json-comments": "^1.0.1",
    "yeoman-generator": "~0.16.0",
    "yosay": "^0.1.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.1",
    "growly": "*",
    "gulp": "*",
    "gulp-jscs": "*",
    "gulp-jshint": "*",
    "gulp-load-plugins": "*",
    "jshint-stylish": "*",
    "karma-growl-reporter": "*",
    "map-stream": "*",
    "mocha": "*",
    "mocha-lcov-reporter": "0.0.1",
    "mockery": "^1.4.0",
    "require-dir": "*",
    "stream-combiner": "*"
  },
  "peerDependencies": {
    "yo": ">=1.0.0"
  }
}
