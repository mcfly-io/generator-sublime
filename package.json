{
  "name": "generator-sublime",
  "version": "1.0.3",
  "description": "Yeoman generator for scaffolfding the standard configuration root files like .gitignore, .jshintrc, .jscsrc etc...",
  "license": "MIT",
  "main": "app/index.js",
  "repository": "thaiat/generator-sublime",
  "author": {
    "name": "Avi Haiat",
    "email": "thaiat@ipelia.com",
    "url": "https://github.com/thaiat"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "prepublish": "bash ./deploy/prepublish.sh",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha ./test && istanbul report cobertura",
    "test2": "gulp lint && mocha ./test",
    "test": "gulp lint &&  istanbul cover ./node_modules/mocha/bin/_mocha ./test && istanbul report cobertura"
  },
  "files": [
    "app",
    "templates"
  ],
  "preferGlobal": true,
  "keywords": [
    "yeoman-generator",
    "editor",
    "app",
    "sublime",
    "lint",
    "jshint",
    "jsbeautify",
    "tern",
    "scaffold"
  ],
  "dependencies": {
    "chalk": "^0.5.1",
    "github": "^0.2.2",
    "lodash": "^2.4.1",
    "strip-json-comments": "^1.0.1",
    "yeoman-generator": "^0.17.0",
    "yosay": "^0.3.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.1",
    "growly": "^1.1.1",
    "gulp": "^3.8.7",
    "gulp-eslint": "^0.1.8",
    "gulp-jscs": "^1.1.0",
    "gulp-jshint": "^1.8.4",
    "gulp-load-plugins": "^0.6.0",
    "gulp-util": "^3.0.1",
    "jshint-stylish": "^0.4.0",
    "karma-growl-reporter": "^0.1.1",
    "map-stream": "^0.1.0",
    "mocha": "^1.21.4",
    "mocha-lcov-reporter": "0.0.1",
    "mockery": "^1.4.0",
    "require-dir": "^0.1.0",
    "stream-combiner": "^0.2.1"
  },
  "peerDependencies": {
    "yo": ">=1.0.0"
  }
}
